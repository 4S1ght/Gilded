{
    "compilerOptions": {
      "target": "es2022",
      "module": "es2022",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "outDir": "build",
      "sourceMap": true
    },
    "include": ["src"]
  }
  